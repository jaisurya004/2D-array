// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i,j,row,col;
scanf("%d %d",&row,&col);
int a[row][col];
for(i=0;i<row;i++){
    for(j=0;j<col;j++){
        scanf("%d",&a[i][j]);
    }
}
for(i=0;i<row;i++){
    int min=a[i][0];
    for(j=0;j<col;j++){
    if(min>a[i][j])
    min=a[i][j];
    }
    printf("%d",min);
    printf("\n");
}
    return 0;
}
