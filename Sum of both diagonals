#include <stdio.h>

int main()
{
   int row,col,sum=0,r;
   scanf("%d %d",&row,&col);
   int a[row][col];
   
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++){
           scanf("%d",&a[i][j]);
       }
   }
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++){
           if(i==j)
           sum=sum+a[i][j];
           }
           printf("\n");
    }
   printf("%d",sum);
   sum=0;
   
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++){
           if(i+j==row-1)
           sum=sum+a[i][j];
           }
           
    }
    printf("\n");
   printf("%d",sum);
   
  

    return 0;
}
