// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i,j,row,col,min;
scanf("%d %d",&row,&col);
int a[row][col];
for(i=0;i<row;i++){
    for(j=0;j<col;j++){
        scanf("%d",&a[i][j]);
    }
}
for(i=0;i<row;i++){
    min=a[i][0];
    for(j=0;j<col;j++){
    if(min>a[j][i])
    min=a[j][i];
    }
    printf("%d",min);
    printf("\n");
}
    return 0;
}
