#include <stdio.h>

int main()
{
   int row,col,max=0,r;
   scanf("%d %d",&row,&col);
   int a[row][col];
   
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++){
           scanf("%d",&a[i][j]);
       }
   }
   for(int i=0;i<row;i++){
       max=a[i][i+1];
       for(int j=0;j<col;j++){
           if(max<a[i][j])
           max=a[i][j];
           
        }
        printf("%d",max);
       printf("\n");
       
       
   }
  

    return 0;
}
